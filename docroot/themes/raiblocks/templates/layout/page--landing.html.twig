        </ul>
      </div>
      </nav>
    </div>
  </div>
</div>

<header class="main-header">

  <!-- Start: Banner -->
    {% if page.banner %}
      <div class="banner">
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              {{ page.banner }}
            </div>
          </div>
        </div>
      </div>
    {% endif %}
  <!-- End: Banner -->

  <!--- Start Messages -->
    {% if page.messages %}
        <div class="container">
          <div class="row">
            <div class="col-md-12">
              {{ page.messages }}
            </div>
          </div>
      </div>
    {% endif %}
  <!---End Messages -->


</header>

  <!-- start container -->

      <!--- Start content -->
      {% if page.content %}
          <div class="col-md-12">
            {{ page.content }}
          </div>
      {% endif %}
      <!---End content -->

  <!--end container -->

  <div class="footer text-center">
