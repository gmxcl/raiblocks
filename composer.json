{
    "name": "translector/realmedianetwork",
    "description": "Real Media Network website",
    "type": "project",
    "require": {
        "cweagans/composer-patches": "^1.6.0",
        "drupal/admin_toolbar": "^1.24",
        "drupal/backup_migrate": "^4.0",
        "drupal/bg_image_formatter": "^1.2",
        "drupal/ckeditor_bootstrap_buttons": "^1.1",
        "drupal/classy_paragraphs": "^1.0@beta",
        "drupal/config_installer": "^1.8",
        "drupal/console": "^1.0.2",
        "drupal/core": "^8.5",
        "drupal/devel": "^1.2",
        "drupal/entity_browser": "^1.5",
        "drupal/extlink": "^1.1",
        "drupal/field_group": "^1.0",
        "drupal/google_analytics": "^2.2",
        "drupal/honeypot": "^1.27",
        "drupal/paragraphs": "^1.3",
        "drupal/pathauto": "^1.2",
        "drupal/token": "^1.3",
        "drupal/webform": "^5.0@RC",
        "drupal/webform_layout_container": "^1.1",
        "drush/drush": "^9.3",
        "hussainweb/drupal-composer-helper": "^1.0"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsstream": "~1.2",
        "phpunit/phpunit": ">=4.8.28 <5",
        "symfony/css-selector": "~2.8"
    },
    "authors": [
        {
            "name": "Jorge Melo",
            "email": "jorgedjma@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "drupal-composer-helper": {
            "web-prefix": "docroot"
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ]
        }
    },
    "prefer-stable": true,
    "conflict": {
        "drupal/drupal": "*"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
